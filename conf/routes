# This file defines all module routes (Higher priority routes first)
#
# import these routes in the main app as :
# *     /                       module:play-messages
#
# ~~~~

GET    /@messages                controllers.MessagesController.index()
POST   /@messages/update         controllers.MessagesController.save()
POST   /@messages/sources/:key   controllers.MessagesController.sources(key: String)
